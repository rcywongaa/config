{
  "workbench.startupEditor": "none",
  "C_Cpp.autocomplete": "default",
  "C_Cpp.formatting": "clangFormat",
  "C_Cpp.errorSquiggles": "enabledIfIncludesResolve",
  "C_Cpp.intelliSenseEngine": "disabled",
  "C_Cpp.intelliSenseEngineFallback": "enabled",
  "editor.wordWrap": "off",
  "editor.occurrencesHighlight": "singleFile",
  "editor.wordSeparators": "`~!@#$%^&*()-=+[{]}\\|;:'\",.<>/?",
  "editor.insertSpaces": true,
  "editor.tabSize": 2,
  "editor.detectIndentation": true,
  "editor.formatOnSave": true,
  "editor.formatOnSaveMode": "modifications",
  "editor.formatOnType": true,
  "editor.formatOnPaste": true,
  "editor.acceptSuggestionOnEnter": "off",
  "editor.selectionClipboard": false,
  "explorer.excludeGitIgnore": true,
  "cmake.configureOnOpen": false,
  "workbench.colorCustomizations": {
    "tab.activeBorder": "#ff0000",
    "tab.unfocusedActiveBorder": "#000000"
  },
  "workbench.editor.enablePreview": true,
  "workbench.editor.navigationScope": "editor",
  "files.trimTrailingWhitespace": true,
  "workbench.editor.titleScrollbarSizing": "large",
  "editor.lineNumbers": "relative",
  "textmarker.enableWholeMatch": true,
  "vim.debug.loggingLevelForConsole": "debug",
  "vim.leader": " ",
  "vim.easymotion": true,
  "vim.camelCaseMotion.enable": true,
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.visualstar": true,
  "vim.ignorecase": false,
  "vim.enableNeovim": false,
  "vim.surround": true,
  "vim.timeout": 500,
  "vim.insertModeKeyBindingsNonRecursive": [
    {
      "before": ["j", "k"],
      "after": ["<Esc>"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["#"],
      "after": ["g", "c", "c"]
    },
    {
      "before": ["f"],
      "after": ["leader", "leader", "s"]
    },
    {
      "before": ["F"],
      "after": ["leader", "leader", "2", "s"]
    },
    {
      "before": ["y", "f"],
      "after": ["y", "leader", "leader", "s"]
    },
    {
      "before": ["y", "l"],
      "after": ["^", "y", "$"]
    },
    {
      "before": ["D"],
      "after": ["d"]
    },
    {
      "before": ["D", "D"],
      "after": ["d", "d"]
    },
    {
      "before": ["d"],
      "after": ["\"", "_", "d"]
    },
    {
      "before": ["r", "("],
      "after": ["%", "r", ")", "`", "`", "r", "("]
    },
    {
      "before": ["r", ")"],
      "after": ["%", "r", "(", "`", "`", "r", ")"]
    },
    {
      "before": ["r", "["],
      "after": ["%", "r", "]", "`", "`", "r", "["]
    },
    {
      "before": ["r", "]"],
      "after": ["%", "r", "[", "`", "`", "r", "]"]
    },
    {
      "before": ["r", "{"],
      "after": ["%", "r", "}", "`", "`", "r", "{"]
    },
    {
      "before": ["r", "}"],
      "after": ["%", "r", "{", "`", "`", "r", "}"]
    },
    {
      "before": ["r", "<"],
      "after": ["%", "r", ">", "`", "`", "r", "<"]
    },
    {
      "before": ["r", ">"],
      "after": ["%", "r", "<", "`", "`", "r", ">"]
    },
    {
      "before": ["d", "("],
      "after": ["<plugds>", "("]
    },
    {
      "before": ["d", ")"],
      "after": ["<plugds>", ")"]
    },
    {
      "before": ["d", "{"],
      "after": ["<plugds>", "{"]
    },
    {
      "before": ["d", "}"],
      "after": ["<plugds>", "}"]
    },
    {
      "before": ["d", "["],
      "after": ["<plugds>", "["]
    },
    {
      "before": ["d", "]"],
      "after": ["<plugds>", "]"]
    },
    {
      "before": ["d", "\""],
      "after": ["<plugds>", "\""]
    },
    {
      "before": ["d", "<"],
      "after": ["<plugds>", ">"]
    },
    {
      "before": ["d", ">"],
      "after": ["<plugds>", ">"]
    },
    {
      "before": ["x"],
      "after": ["\"", "_", "x"]
    },
    {
      "before": ["w"],
      "after": ["<leader>", "w"]
    },
    {
      "before": ["W"],
      "after": ["w"]
    },
    {
      "before": ["b"],
      "after": ["<leader>", "b"]
    },
    {
      "before": ["B"],
      "after": ["b"]
    },
    {
      "before": ["e"],
      "after": ["<leader>", "e"]
    },
    {
      "before": ["E"],
      "after": ["e"]
    },
    {
      "before": ["<leader>", "J"],
      "after": ["J"]
    },
    {
      "before": ["J"],
      "after": []
    },
    {
      "before": ["H"],
      "after": []
    },
    {
      "before": ["L"],
      "after": []
    },
    {
      "before": ["K"],
      "after": []
    },
    {
      "before": ["*"],
      "after": ["*", "N", "z", "z"]
    },
    {
      "before": ["<leader>", "*"],
      "commands": ["textmarker.toggleHighlight"]
    },
    // {
    //   "before": ["n"],
    //   "commands": ["textmarker.goToNextHighlight"]
    // },
    // {
    //   "before": ["N"],
    //   "commands": ["textmarker.goToPreviousHighlight"]
    // },
    {
      "before": ["<leader>", "<enter>"],
      "commands": [":e!"]
    },
    {
      "before": ["/", "/"],
      "commands": [":nohl"]
    },
    {
      "before": ["<leader>", "m"],
      "commands": ["bookmarks.toggle"]
    },
    {
      "before": ["<leader>", "b"],
      "commands": ["bookmarks.list"]
    },
    {
      "before": ["<leader>", "w"],
      "commands": ["workbench.action.files.save"]
    },
    {
      "before": ["<leader>", "q"],
      "commands": ["workbench.action.closePanel"],
      "when": "panelFocus"
    },
    {
      "before": ["<leader>", "q"],
      "commands": ["workbench.action.closeActiveEditor"],
      "when": "editorFocus"
    },
    {
      "before": [";", "s", "p"],
      "commands": ["workbench.action.splitEditorRight"]
    },
    {
      "before": [";", "b", "s", "p"],
      "commands": ["workbench.action.splitEditorDown"]
    },
    {
      "before": ["="],
      "commands": ["workbench.action.nextEditorInGroup"]
    },
    {
      "before": ["-"],
      "commands": ["workbench.action.previousEditorInGroup"]
    },
    {
      "before": ["<C-k>"],
      "commands": ["workbench.action.navigateUp"]
    },
    {
      "before": ["<C-j>"],
      "commands": ["workbench.action.navigateDown"]
    },
    {
      "before": ["<C-h>"],
      "commands": ["workbench.action.navigateLeft"]
    },
    {
      "before": ["<C-l>"],
      "commands": ["workbench.action.navigateRight"]
    },
    {
      "before": ["u"],
      "commands": ["undo"]
    },
    {
      "before": ["<C-r>"],
      "commands": ["redo"]
    },
    {
      "before": ["<C-o>"],
      "commands": ["workbench.action.navigateBack"]
    },
    {
      "before": ["<C-i>"],
      "commands": ["workbench.action.navigateForward"]
    },
    {
      "before": ["_"],
      "commands": [
        {
          "command": "moveActiveEditor",
          "args": {
            "to": "left"
          }
        }
      ]
    },
    {
      "before": ["+"],
      "commands": [
        {
          "command": "moveActiveEditor",
          "args": {
            "to": "right"
          }
        }
      ]
    },
    {
      "before": ["("],
      "after": []
    },
    {
      "before": [")"],
      "after": []
    },
  ],
